<?xml version="1.0" encoding="utf-8" ?>
<Root>
  <!-- Facet overlays. -->

  <SimpleBodyFacet>
    <BodyType>Dynamic</BodyType>
    <Density>10</Density>
    <Friction>0</Friction>
    <Restitution>0</Restitution>
    <FixedRotation>False</FixedRotation>
    <LinearDamping>1</LinearDamping>
    <AngularDamping>1</AngularDamping>
    <GravityScale>1</GravityScale>
    <CollisionCategories>1</CollisionCategories>
    <CollisionMask>*</CollisionMask>
    <IsBullet>False</IsBullet>
    <IsSensor>False</IsSensor>
  </SimpleBodyFacet>

  <SimpleSpriteFacet/>

  <SimpleAnimatedSpriteFacet/>

  <!-- Dispatcher overlays. -->

  <EntityDispatcher>
    <Visible>True</Visible>
  </EntityDispatcher>

  <Entity2dDispatcher>
    <Include>EntityDispatcher</Include>
    <Rotation>0</Rotation>
  </Entity2dDispatcher>

  <GuiDispatcher>
    <Include>Entity2dDispatcher</Include>
    <Enabled>True</Enabled>
  </GuiDispatcher>

  <SimpleBodyDispatcher>
    <Include>Entity2dDispatcher; SimpleBodyFacet</Include>
  </SimpleBodyDispatcher>

  <SimpleBodySpriteDispatcher>
    <Include>SimpleBodyDispatcher; SimpleSpriteFacet</Include>
  </SimpleBodySpriteDispatcher>

  <SimpleBodyAnimatedSpriteDispatcher>
    <Include>SimpleBodyDispatcher; SimpleAnimatedSpriteFacet</Include>
  </SimpleBodyAnimatedSpriteDispatcher>

  <ButtonDispatcher>
    <Include>GuiDispatcher</Include>
    <IsDown>False</IsDown>
    <ClickSound>Sound;Default;AssetGraph.xml</ClickSound>
  </ButtonDispatcher>

  <LabelDispatcher>
    <Include>GuiDispatcher</Include>
  </LabelDispatcher>

  <TextBoxDispatcher>
    <Include>GuiDispatcher</Include>
    <TextColor>1; 1; 1; 1</TextColor>
    <TextFont>Font;Default;AssetGraph.xml</TextFont>
    <TextOffset>0;0</TextOffset>
  </TextBoxDispatcher>

  <ToggleDispatcher>
    <Include>GuiDispatcher</Include>
    <IsPressed>False</IsPressed>
  </ToggleDispatcher>

  <FeelerDispatcher>
    <Include>GuiDispatcher</Include>
    <IsTouched>False</IsTouched>
  </FeelerDispatcher>

  <FillBarDispatcher>
    <Include>GuiDispatcher</Include>
    <Fill>0</Fill>
    <FillInset>0</FillInset>
  </FillBarDispatcher>

  <BlockDispatcher>
    <Include>SimpleBodySpriteDispatcher</Include>
  </BlockDispatcher>

  <AvatarDispatcher>
    <Include>SimpleBodySpriteDispatcher</Include>
    <FixedRotation>True</FixedRotation>
    <LinearDamping>10</LinearDamping>
    <GravityScale>0</GravityScale>
  </AvatarDispatcher>

  <CharacterDispatcher>
    <Include>SimpleBodySpriteDispatcher</Include>
    <FixedRotation>True</FixedRotation>
    <LinearDamping>3</LinearDamping>
  </CharacterDispatcher>

  <TileMapDispatcher>
    <Include>Entity2dDispatcher</Include>
    <Density>10</Density>
    <Friction>0</Friction>
    <Restitution>0</Restitution>
    <CollisionCategories>1</CollisionCategories>
    <CollisionMask>*</CollisionMask>
    <Parallax>0</Parallax>
  </TileMapDispatcher>

</Root>