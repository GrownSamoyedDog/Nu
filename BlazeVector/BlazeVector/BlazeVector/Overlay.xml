<?xml version="1.0" encoding="utf-8" ?>
<Root>

  <EntityOverlay>
    <Visible>True</Visible>
  </EntityOverlay>

  <Entity2dOverlay>
    <Include>EntityOverlay</Include>
  </Entity2dOverlay>

  <GuiOverlay>
    <Include>Entity2dOverlay</Include>
    <Enabled>True</Enabled>
    <Rotation>0</Rotation>
  </GuiOverlay>

  <SimpleBodyOverlay>
    <Include>Entity2dOverlay</Include>
    <BodyType>Dynamic</BodyType>
    <Density>10</Density>
    <Friction>0</Friction>
    <Restitution>0</Restitution>
    <FixedRotation>False</FixedRotation>
    <LinearDamping>1</LinearDamping>
    <AngularDamping>1</AngularDamping>
    <GravityScale>1</GravityScale>
    <CollisionCategories>1</CollisionCategories>
    <CollisionMask>*</CollisionMask>
    <IsBullet>False</IsBullet>
    <IsSensor>False</IsSensor>
  </SimpleBodyOverlay>

  <SimpleSpriteOverlay>
    <Include>Entity2dOverlay</Include>
  </SimpleSpriteOverlay>

  <SimpleAnimatedSpriteOverlay>
    <Include>Entity2dOverlay</Include>
  </SimpleAnimatedSpriteOverlay>

  <SimpleBodySpriteOverlay>
    <Include>SimpleBodyOverlay; SimpleSpriteOverlay</Include>
  </SimpleBodySpriteOverlay>

  <SimpleBodyAnimatedSpriteOverlay>
    <Include>SimpleBodyOverlay; SimpleAnimatedSpriteOverlay</Include>
  </SimpleBodyAnimatedSpriteOverlay>

  <ButtonOverlay>
    <Include>GuiOverlay</Include>
    <IsDown>False</IsDown>
    <ClickSound>Sound;Default;AssetGraph.xml</ClickSound>
  </ButtonOverlay>

  <LabelOverlay>
    <Include>GuiOverlay</Include>
  </LabelOverlay>

  <TextBoxOverlay>
    <Include>GuiOverlay</Include>
    <TextColor>1; 1; 1; 1</TextColor>
    <TextFont>Font;Default;AssetGraph.xml</TextFont>
    <TextOffset>0;0</TextOffset>
  </TextBoxOverlay>

  <ToggleOverlay>
    <Include>GuiOverlay</Include>
    <IsPressed>False</IsPressed>
  </ToggleOverlay>

  <FeelerOverlay>
    <Include>GuiOverlay</Include>
    <IsTouched>False</IsTouched>
  </FeelerOverlay>

  <FillBarOverlay>
    <Include>GuiOverlay</Include>
    <Fill>0</Fill>
    <FillInset>0</FillInset>
  </FillBarOverlay>

  <BlockOverlay>
    <Include>SimpleBodySpriteOverlay</Include>
  </BlockOverlay>

  <AvatarOverlay>
    <Include>SimpleBodySpriteOverlay</Include>
    <FixedRotation>True</FixedRotation>
    <LinearDamping>10</LinearDamping>
    <GravityScale>0</GravityScale>
  </AvatarOverlay>

  <CharacterOverlay>
    <Include>SimpleBodySpriteOverlay</Include>
    <FixedRotation>True</FixedRotation>
    <LinearDamping>3</LinearDamping>
  </CharacterOverlay>

  <TileMapOverlay>
    <Include>Entity2dOverlay</Include>
    <Density>10</Density>
    <Friction>0</Friction>
    <Restitution>0</Restitution>
    <CollisionCategories>1</CollisionCategories>
    <CollisionMask>*</CollisionMask>
    <Parallax>0</Parallax>
  </TileMapOverlay>

</Root>