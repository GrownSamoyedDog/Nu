<?xml version="1.0" encoding="utf-8" ?>
<Root>
  <!-- Facet overlays. -->

  <SimpleBodyFacet>
    <BodyType>Dynamic</BodyType>
    <Density>10</Density>
    <Friction>0</Friction>
    <Restitution>0</Restitution>
    <FixedRotation>False</FixedRotation>
    <LinearDamping>1</LinearDamping>
    <AngularDamping>1</AngularDamping>
    <GravityScale>1</GravityScale>
    <CollisionCategories>1</CollisionCategories>
    <CollisionMask>*</CollisionMask>
    <IsBullet>False</IsBullet>
    <IsSensor>False</IsSensor>
  </SimpleBodyFacet>

  <SimpleSpriteFacet/>

  <SimpleAnimatedSpriteFacet/>

  <!-- Dispatcher overlays. -->

  <EntityDispatcher>
    <Visible>True</Visible>
  </EntityDispatcher>

  <Entity2dDispatcher include="EntityDispatcher">
    <Rotation>0</Rotation>
  </Entity2dDispatcher>

  <GuiDispatcher include="Entity2dDispatcher">
    <Enabled>True</Enabled>
  </GuiDispatcher>

  <SimpleBodyDispatcher include="Entity2dDispatcher;SimpleBodyFacet" />

  <SimpleBodySpriteDispatcher include="SimpleBodyDispatcher;SimpleSpriteFacet" />

  <SimpleBodyAnimatedSpriteDispatcher include="SimpleBodyDispatcher;SimpleAnimatedSpriteFacet" />

  <ButtonDispatcher include="GuiDispatcher">
    <IsDown>False</IsDown>
    <ClickSound>Sound;Default;AssetGraph.xml</ClickSound>
  </ButtonDispatcher>

  <LabelDispatcher include="GuiDispatcher" />

  <TextDispatcher include="GuiDispatcher">
    <TextColor>1;1;1;1</TextColor>
    <TextFont>Font;Default;AssetGraph.xml</TextFont>
    <TextOffset>0;0</TextOffset>
  </TextDispatcher>

  <ToggleDispatcher include="GuiDispatcher">
    <IsPressed>False</IsPressed>
  </ToggleDispatcher>

  <FeelerDispatcher include="GuiDispatcher">
    <Include></Include>
    <IsTouched>False</IsTouched>
  </FeelerDispatcher>

  <FillBarDispatcher include="GuiDispatcher">
    <Fill>0</Fill>
    <FillInset>0</FillInset>
  </FillBarDispatcher>

  <BlockDispatcher include="SimpleBodySpriteDispatcher">
  </BlockDispatcher>

  <AvatarDispatcher include="SimpleBodySpriteDispatcher">
    <FixedRotation>True</FixedRotation>
    <LinearDamping>10</LinearDamping>
    <GravityScale>0</GravityScale>
  </AvatarDispatcher>

  <CharacterDispatcher include="SimpleBodySpriteDispatcher">
    <FixedRotation>True</FixedRotation>
    <LinearDamping>3</LinearDamping>
  </CharacterDispatcher>

  <TileMapDispatcher include="Entity2dDispatcher">
    <Density>10</Density>
    <Friction>0</Friction>
    <Restitution>0</Restitution>
    <CollisionCategories>1</CollisionCategories>
    <CollisionMask>*</CollisionMask>
    <Parallax>0</Parallax>
  </TileMapDispatcher>

</Root>